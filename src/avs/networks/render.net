    panel "integer to file.user.15" -w panel -p "read parflow macro.user.13" \
        -xy 0,63 -wh 256,239
     manipulator "integer to file.user.15:File Prefix" -w browser -p "integer to file.user.15" \
         -xy 9,9 -wh 237,199
     manipulator "integer to file.user.15:Format" -w typein -p "integer to file.user.15" \
         -xy 9,206 -wh 237,22
    panel "read parflow.user.16" -w panel -p "read parflow macro.user.13" \
        -xy 0,108 -wh 256,285
     manipulator "read parflow.user.16:File Browser" -w browser -p "read parflow.user.16" \
         -xy 9,9 -wh 237,199
     manipulator "read parflow.user.16:Scan Files" -w radio_buttons -p "read parflow.user.16" \
         -xy 9,206 -wh 118,67
 panel page.0 -w panel -p ui -xy 451,75 -wh 688,850
  panel "geometry viewer.user.6!display" -w container -p page.0 \
      -xy 21,8 -wh 649,678\
   -P zoom_coords string "1 0 0 0 0 <page.0> 0 0 0 0"
  panel page.1 -w panel -p page.0 -xy 143,692 -wh 135,141\
   -P title string "Axes Controls"
   manipulator "generate axes.user.7:Regenerate" -w oneshot -p page.1 \
       -xy 8,46 -wh 118,22
   manipulator "generate axes.user.7:Tick Marks" -w toggle -p page.1 \
       -xy 8,75 -wh 118,22
   manipulator "generate axes.user.7:Tick Labels" -w toggle -p page.1 \
       -xy 8,108 -wh 118,22
  panel downsize.user.1 -w panel -p page.0 -xy 457,692 -wh 109,147
   manipulator downsize.user.1:downsize -w idial -p downsize.user.1 \
       -xy 9,9 -wh 90,129
manipulator page.1!title -w text -p page.1 -xy 8,3 -wh 118,22

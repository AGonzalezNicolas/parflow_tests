#!/usr/bin/avs -network
version 5.02 (50.62 SunOS4)
module "color range.user.0" -xy 165,201 -ex $Path/avs_library/mongo
module downsize.user.1 -xy 349,113 -ex $Path/avs_library/mongo
module "generate colormap.user.2" -xy 165,97
module colorizer.user.3 -xy 165,273 -ex $Path/avs_library/mongo

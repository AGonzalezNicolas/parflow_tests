       -xy 0,108 -wh 256,394
    panel "animated integer.user.14" -w panel -p "read parflow macro.user.13" \
        -xy 0,63 -wh 256,173
     manipulator "animated integer.user.14:min value" -w typein_integer -p "animated integer.user.14" \
         -xy 9,9 -wh 237,22
     manipulator "animated integer.user.14:max value" -w typein_integer -p "animated integer.user.14" \
         -xy 9,29 -wh 237,22

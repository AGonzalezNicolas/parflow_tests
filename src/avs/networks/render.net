module "read parflow macro.user.13" -xy 349,49 -macro -type 1
port_add -out "read parflow macro.user.13":"Uniform Output" "field 3D scalar 3-coord uniform float" -flags 0x2
port_add -out "read parflow macro.user.13":"Scattered Output" "field 1D scalar 3-coord irregular float" -flags 0x2
port_add -out "read parflow macro.user.13":"integer value" integer -flags 0x2
module "animated integer.user.14" -xy 301,121 -ex $Path/avs_library/anim_int -parent "read parflow macro.user.13"
module "integer to file.user.15" -xy 301,185 -ex /home/bacchus0/falgout/parflow/exe.SunOS/avs/mod_lib/pf_modules -parent "read parflow macro.user.13"
module "read parflow.user.16" -xy 301,249 -ex /home/bacchus0/falgout/parflow/exe.SunOS/avs/mod_lib/pf_modules -parent "read parflow macro.user.13"
port_vis "read parflow.user.16":"File Browser" -on
port_connect "color range.user.0":0 colorizer.user.3:1
port_connect downsize.user.1:0 "color range.user.0":0
port_connect downsize.user.1:0 colorizer.user.3:0
port_connect downsize.user.1:0 "volume render.user.4":0
port_connect downsize.user.1:0 "volume bounds.user.5":0
port_connect "generate colormap.user.2":0 "color range.user.0":1
port_connect colorizer.user.3:0 "geometry viewer.user.6":1
port_connect "volume render.user.4":0 "geometry viewer.user.6":0
port_connect "volume bounds.user.5":0 "geometry viewer.user.6":0
port_connect "geometry viewer.user.6":0 "image to postscript.user.8":0
port_connect "generate axes.user.7":0 "geometry viewer.user.6":0
port_connect "read parflow macro.user.13":0 downsize.user.1:0
port_connect "animated integer.user.14":0 "OUT-> read parflow macro.user.13":2
port_connect "animated integer.user.14":0 "integer to file.user.15":0
port_connect "integer to file.user.15":0 "read parflow.user.16":0
port_connect "read parflow.user.16":0 "OUT-> read parflow macro.user.13":0
port_connect "read parflow.user.16":1 "OUT-> read parflow macro.user.13":1
parm_set downsize.user.1:downsize 1
geom_set_scene -scene "geometry viewer.user.6"
geom_set_camera_name "Camera 1"
geom_resize_camera -view "Camera 1" 512 512
#
# State for view: Camera 1
#
geom_set_position -view "Camera 1" 0 0 -12
geom_set_view_modes -depth_cue 0 -view "Camera 1"
geom_set_view_modes -polygonal_spheres 0 -view "Camera 1"
geom_set_view_modes -stereo 0 -view "Camera 1"
geom_set_view_modes -head_tracking 1 -view "Camera 1"
geom_set_view_modes -z_buffer 1 -view "Camera 1"
geom_set_camera_params -view "Camera 1" -front -88 -back 112
geom_set_depth_cue_params "Camera 1" -scale 0.1
#
# Light state
#
geom_set_light -light 1 -type directional -state 1
geom_set_light -type ambient -state 1
#
# State for object: top
#
geom_set_cur_cli_obj top
geom_set_matrix   -mat \
                  0.30876    0.0505526    -0.127131    0 \
                    -0.00592712    0.318462    0.112239    0 \
                    0.136684    -0.100385    0.292046    0 \
                    -4.39518    -2.68629    -2.77154    1 
geom_set_obj_window 0 20 0 20 0 20
#
# State for object: "volume render"
#
geom_set_cur_cli_obj -push
geom_set_name_context "volume render.user.4"
geom_create_obj "volume render" -mod "volume render.user.4"
geom_set_trans_mode parent
geom_set_obj_window 0 20 0 20 0 20
geom_set_name_context
geom_set_cur_cli_obj -pop
#
# State for object: bounds
#
geom_set_cur_cli_obj -push
geom_set_name_context "volume bounds.user.5"
geom_create_obj bounds -mod "volume bounds.user.5"
geom_set_trans_mode parent
geom_set_obj_window 0 20 0 20 0 20
geom_set_name_context
geom_set_cur_cli_obj -pop
#
# State for object: axes
#
geom_set_cur_cli_obj -push
geom_set_name_context "generate axes.user.7"
geom_create_obj axes -mod "generate axes.user.7"
geom_set_trans_mode parent
geom_set_name_context
geom_set_cur_cli_obj -pop

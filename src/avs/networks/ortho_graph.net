#!/usr/bin/avs -network
version 5.02 (50.62 SunOS4)
module "orthogonal slicer.user.0" -xy 13,89 -ex $Path/avs_library/mongo
module "graph viewer.user.1" -xy 13,161
module statistics.user.2 -xy 149,89 -ex $Path/avs_library/mongo
module "read parflow macro.user.7" -xy 13,25 -macro -type 1
port_add -out "read parflow macro.user.7":"Uniform Output" "field 3D scalar 3-coord uniform float" -flags 0x2
port_add -out "read parflow macro.user.7":"Scattered Output" "field 1D scalar 3-coord irregular float" -flags 0x2
port_add -out "read parflow macro.user.7":"integer value" integer -flags 0x2
module "animated integer.user.8" -xy 301,121 -ex $Path/avs_library/anim_int -parent "read parflow macro.user.7"
module "integer to file.user.9" -xy 301,185 -ex /home/bacchus0/falgout/parflow/exe.SunOS/avs/mod_lib/pf_modules -parent "read parflow macro.user.7"
module "read parflow.user.10" -xy 301,249 -ex /home/bacchus0/falgout/parflow/exe.SunOS/avs/mod_lib/pf_modules -parent "read parflow macro.user.7"
port_vis "read parflow.user.10":"File Browser" -on
port_connect "orthogonal slicer.user.0":0 "graph viewer.user.1":1
port_connect "read parflow macro.user.7":0 statistics.user.2:0
port_connect "read parflow macro.user.7":0 "orthogonal slicer.user.0":0
port_connect "animated integer.user.8":0 "OUT-> read parflow macro.user.7":2
port_connect "animated integer.user.8":0 "integer to file.user.9":0
port_connect "integer to file.user.9":0 "read parflow.user.10":0
port_connect "read parflow.user.10":0 "OUT-> read parflow macro.user.7":0
port_connect "read parflow.user.10":1 "OUT-> read parflow macro.user.7":1
parm_set "orthogonal slicer.user.0":axis J
mod_graph "graph viewer.user.1"

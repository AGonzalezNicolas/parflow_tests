        -xy 9,9 -wh 118,22
    manipulator statistics.user.2:Statistics -w textblock -p statistics.user.2 \
        -xy 9,29 -wh 237,201
   panel "read parflow macro.user.7" -w stack -p "Top Level Stack" \
       -xy 0,108 -wh 256,394
    panel "animated integer.user.8" -w panel -p "read parflow macro.user.7" \
        -xy 0,63 -wh 256,173

shell "ui" shell
 panel Application -w app_panel -p ui -xy 0,0 -wh 259,1023
  panel "Top Level Stack" -w master_stack -p Application -xy 2,100 -wh 256,502
   panel "orthogonal slicer.user.0" -w panel -p "Top Level Stack" \
       -xy 0,63 -wh 227,147

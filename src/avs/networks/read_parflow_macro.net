#!/usr/bin/avs -network
version 5.02 (50.62 SunOS4)
module "read parflow macro.user.0" -xy 84,85 -macro -type 1
port_add -out "read parflow macro.user.0":"Uniform Output" "field 3D scalar 3-coord uniform float" -flags 0x2
port_add -out "read parflow macro.user.0":"Scattered Output" "field 1D scalar 3-coord irregular float" -flags 0x2
port_add -out "read parflow macro.user.0":"integer value" integer -flags 0x2
module "animated integer.user.1" -xy 301,121 -ex $Path/avs_library/anim_int -parent "read parflow macro.user.0"
module "integer to file.user.2" -xy 301,185 -ex /home/bacchus0/falgout/parflow/exe.SunOS/avs/mod_lib/pf_modules -parent "read parflow macro.user.0"
module "read parflow.user.3" -xy 301,249 -ex /home/bacchus0/falgout/parflow/exe.SunOS/avs/mod_lib/pf_modules -parent "read parflow macro.user.0"
port_vis "read parflow.user.3":"File Browser" -on
port_connect "animated integer.user.1":0 "integer to file.user.2":0
port_connect "animated integer.user.1":0 "OUT-> read parflow macro.user.0":2
port_connect "integer to file.user.2":0 "read parflow.user.3":0
port_connect "read parflow.user.3":0 "OUT-> read parflow macro.user.0":0
port_connect "read parflow.user.3":1 "OUT-> read parflow macro.user.0":1
shell "ui" shell
 panel Application -w app_panel -p ui -xy 0,0 -wh 259,1023
  panel "Top Level Stack" -w master_stack -p Application -xy 2,100 -wh 256,457
   panel "read parflow macro.user.0" -w stack -p "Top Level Stack" \
       -xy 0,63 -wh 256,394
    panel "animated integer.user.1" -w panel -p "read parflow macro.user.0" \
        -xy 0,63 -wh 256,173
     manipulator "animated integer.user.1:min value" -w typein_integer -p "animated integer.user.1" \
         -xy 9,9 -wh 237,22
     manipulator "animated integer.user.1:max value" -w typein_integer -p "animated integer.user.1" \
         -xy 9,29 -wh 237,22
     manipulator "animated integer.user.1:steps" -w typein_integer -p "animated integer.user.1" \
         -xy 9,54 -wh 118,22
     manipulator "animated integer.user.1:sleep" -w toggle -p "animated integer.user.1" \
         -xy 9,75 -wh 118,22
     manipulator "animated integer.user.1:choice" -w radio_buttons -p "animated integer.user.1" \
         -xy 9,99 -wh 118,67
    panel "integer to file.user.2" -w panel -p "read parflow macro.user.0" \
        -xy 0,86 -wh 256,239
     manipulator "integer to file.user.2:File Prefix" -w browser -p "integer to file.user.2" \
         -xy 9,9 -wh 237,199
     manipulator "integer to file.user.2:Format" -w typein -p "integer to file.user.2" \
         -xy 9,206 -wh 237,22
    panel "read parflow.user.3" -w panel -p "read parflow macro.user.0" \
        -xy 0,108 -wh 256,285
     manipulator "read parflow.user.3:File Browser" -w browser -p "read parflow.user.3" \
         -xy 9,9 -wh 237,199
     manipulator "read parflow.user.3:Scan Files" -w radio_buttons -p "read parflow.user.3" \
         -xy 9,206 -wh 118,67
# End of file
